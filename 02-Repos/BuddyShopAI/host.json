{
    "version": "2.0",
    "logging": {
        "logLevel": {
            "default": "Information",
            "Host": "Information",
            "Function": "Information"
        },
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "maxTelemetryItemsPerSecond": 5
            },
            "enableLiveMetricsFilters": true
        }
    },
    "functionTimeout": "00:05:00",
    "extensions": {
        "http": {
            "routePrefix": "api"
        }
    }
}